  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/lib/Bundler/source-map-compare",
      // "args": ["compare", "./.local/baseline/**/*.js", "./.local/compare/**/*.js"],
      "args": [
        "compare",
        "./.local/input/baseline/loop-app-release-bundle/hashed/card-loop-adaptivecardview.fa16901d12b9fcad5c44.js",
        "./.local/input/compare/loop-app-release-bundle/hashed/card-loop-adaptivecardview.14c550538fbf4ab951f9.js"
      ],
      "runtimeArgs": ["--openssl-legacy-provider"],
      "console": "integratedTerminal",
      "request": "launch",
      "preLaunchTask": "npm: build",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "sourceMaps": true
    }
  ]
